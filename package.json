{
  "name": "obytesapp",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "prebuild": "yarn expo prebuild",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "doctor": "expo doctor",
    "start:staging": "APP_ENV=staging expo start --dev-client",
    "prebuild:staging": "APP_ENV=staging yarn expo prebuild",
    "android:staging": "APP_ENV=staging expo run:android",
    "ios:staging": "APP_ENV=staging expo run:ios",
    "start:production": "APP_ENV=production expo start --dev-client",
    "prebuild:production": "APP_ENV=production yarn expo prebuild",
    "android:production": "APP_ENV=production expo run:android",
    "ios:production": "APP_ENV=production expo run:ios",
    "build:development:ios": "APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "APP_ENV=development eas build --profile development --platform android ",
    "build:staging:ios": "APP_ENV=staging eas build --profile staging --platform ios",
    "build:staging:android": "APP_ENV=staging eas build --profile staging --platform android ",
    "build:production:ios": "APP_ENV=production eas build --profile production --platform ios",
    "build:production:android": "APP_ENV=production eas build --profile production --platform android ",
    "postinstall": "husky install",
    "np": "np --no-publish",
    "version": "yarn prebuild && git add .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "dependencies": {
    "@bacons/link-assets": "^1.1.0",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^2.9.8",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.9.1",
    "@shopify/flash-list": "1.3.1",
    "@tanstack/react-query": "^4.3.4",
    "axios": "^0.27.2",
    "expo": "~47.0.12",
    "expo-community-flipper": "^47.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-localization": "~14.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-updates": "~0.15.6",
    "i18next": "^21.9.1",
    "lodash.memoize": "^4.1.2",
    "nativewind": "^2.0.10",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.35.0",
    "react-i18next": "^11.18.5",
    "react-native": "0.70.5",
    "react-native-fast-image": "^8.6.1",
    "react-native-flash-message": "^0.3.1",
    "react-native-flipper": "^0.182.0",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-mmkv": "2.5.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.7",
    "yarn": "^1.22.19",
    "zod": "^3.19.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@expo/config": "^7.0.2",
    "@jamesacarr/eslint-formatter-github-actions": "^0.1.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.1.2",
    "@testing-library/react-native": "^11.3.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^29.2.1",
    "@types/lodash.memoize": "^4.1.7",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.23.0",
    "eslint-plugin-i18n-json": "^3.1.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-expo": "^47.0.0",
    "lint-staged": "^13.0.3",
    "metro-babel-register": "^0.73.3",
    "np": "^7.5.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/obytes/react-native-template-obytes.git"
  }
}
